{
  "tasks": {
    "dev": "rsbuild dev",
    "site:build": "rsbuild build",
    "site:preview": "rsbuild preview",
    "lib:build": "rslib build",
    "lib:inspect": "rslib inspect"
  },
  "imports": {
    "@rsbuild/core": "npm:@rsbuild/core@^1.0.19",
    "@rsbuild/plugin-solid": "npm:@rsbuild/plugin-solid@^1.0.4",
    "@rslib/core": "npm:@rslib/core@^0.0.16",
    "@std/assert": "jsr:@std/assert@1",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "clsx": "npm:clsx@^2.1.1",
    "rslib": "npm:rslib@^0.0.10",
    "solid-js": "npm:solid-js@^1.9.3",
    "tailwind-merge": "npm:tailwind-merge@^2.5.4",
    "tailwindcss": "npm:tailwindcss@^3.4.14"
  },
  "compilerOptions": {
    "isolatedDeclarations": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "precompile",
    "jsxImportSource": "solid-js",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  }
}
